# Variables and Constants in TinyCL
# Demonstrates variable declarations and basic operations

# Variable declarations
var x = 10;
var y = 20;
var name = "Alice";
var flag = true;
var letter = 'A';

# Constant declarations
const PI = 3;
const MAX_SIZE = 100;

# Print variables
print("x = " + x);
print("y = " + y);
print("name = " + name);
print("flag = " + flag);
print("letter = " + letter);
print("PI = " + PI);
print("MAX_SIZE = " + MAX_SIZE);
